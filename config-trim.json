{
    "loaders": [
        {
            "type": "node",
            "name": "node",
            "configuration": {
                "binding.path": "./bin/libnodejs_binding.so"
            }
        }
    ],
    "modules": [
        {
            "name": "api",
            "loader": {
                "name": "node",
                "entrypoint": {
                    "main.path": "./modules/api.js"
                }
            },
            "args": null
        },
        {
            "name": "printer",
            "loader": {
                "name": "node",
                "entrypoint": {
                    "main.path": "./modules/printer.js"
                }
            },
            "args": null
        }
    ],
    "links": [
        {
            "source": "*",
            "sink": "printer"
        }
    ]
}
